<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Application.FemaleMenuActivity.BreedDetail.FemaleBreedDetailScanRfidActivity">

    <TextView
        android:id="@+id/title_TextView_FemaleBreedDetailScanRfidActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:fontFamily="@font/baibold"
        android:text="@string/RfidTitle_TextView"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/TagCode_FemaleBreedDetailScanRfidActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/title_TextView_FemaleBreedDetailScanRfidActivity">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:fontFamily="@font/baibold"
            android:text="@string/TagCode_TextView"
            android:textAlignment="textEnd"
            android:textColor="@color/black"
            android:textSize="15sp" />

        <TextView
            style="@android:style/Widget.EditText"
            android:background="@null"
            android:id="@+id/ShowTagCode_Textview_FemaleBreedDetailScanRfidActivity"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.7"
            android:layout_marginStart="10dp"
            android:autoSizeTextType="none"
            android:ems="10"
            android:fontFamily="@font/bai"
            android:hint="@string/TagCodeHint_TextView"
            android:textAlignment="textStart"
            android:textColor="@color/black"
            android:textSize="15sp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/RFid_FemaleBreedDetailScanRfidActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="16dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TagCode_FemaleBreedDetailScanRfidActivity">


            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.3"
                android:fontFamily="@font/baibold"
                android:text="@string/Rfid_TextView"
                android:textAlignment="textEnd"
                android:textColor="@color/black"
                android:textSize="15sp" />

            <TextView
                style="@android:style/Widget.EditText"
                android:background="@null"
                android:id="@+id/ShowRfid_Textview_FemaleBreedDetailScanRfidActivity"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.7"
                android:layout_marginStart="10dp"
                android:autoSizeTextType="none"
                android:ems="10"
                android:fontFamily="@font/bai"
                android:hint="@string/RfidHint_TextView"
                android:textAlignment="textStart"
                android:textColor="@color/black"
                android:textSize="15sp" />

    </LinearLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/ScanRfid_Button_FemaleBreedDetailScanRfidActivity"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="24dp"
        android:background="#7DCEA0"
        android:fontFamily="@font/baibold"
        android:text="@string/ScanRfid_Button"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/Next_Button_FemaleBreedDetailScanRfidActivity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/RFid_FemaleBreedDetailScanRfidActivity"
        app:layout_constraintVertical_bias="1.0" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/Next_Button_FemaleBreedDetailScanRfidActivity"
        android:layout_width="300dp"
        android:layout_height="40dp"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="36dp"
        android:background="#85C1E9"
        android:fontFamily="@font/baibold"
        android:text="@string/Next_Button"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>